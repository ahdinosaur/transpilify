{
  "name": "transpilify",
  "version": "1.0.3",
  "description": "simple transpiler using browserify transforms",
  "main": "index.js",
  "scripts": {
    "test": "standard && node test/index.js | faucet",
    "example": "./cli.js test/fixtures/actual.js -t [ babelify --presets es2015 ] -t brfs -t glslify"
  },
  "bin": {
    "transpilify": "./cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahdinosaur/transpilify.git"
  },
  "keywords": [
    "source",
    "transpile",
    "transpiler",
    "browserify",
    "transform",
    "stream"
  ],
  "author": "Mikey <michael.williams@enspiral.com> (http://dinosaur.os)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ahdinosaur/transpilify/issues"
  },
  "homepage": "https://github.com/ahdinosaur/transpilify#readme",
  "dependencies": {
    "globby": "^4.0.0",
    "map-limit": "0.0.1",
    "object-assign": "^4.0.1",
    "pump": "^1.0.1",
    "resolve": "^1.1.6",
    "subarg": "^1.0.0"
  },
  "standard": {
    "ignore": [
      "test/fixtures/**"
    ]
  },
  "devDependencies": {
    "babel-preset-es2015": "6.3.13",
    "babelify": "7.2.0",
    "brfs": "1.4.2",
    "concat-stream": "^1.5.1",
    "faucet": "0.0.1",
    "glsl-random": "0.0.5",
    "glslify": "5.0.0",
    "glslify-hex": "^2.0.1",
    "standard": "^5.4.1",
    "tape": "^4.4.0"
  }
}
